    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.2.0",
