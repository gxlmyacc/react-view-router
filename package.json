{
  "name": "react-view-router",
  "version": "2.0.11-alpha.6",
  "description": "react-view-router",
  "keywords": [
    "react-view-router",
    "react",
    "router",
    "vue-router",
    "react-router",
    "react-router-dom"
  ],
  "sideEffects": false,
  "main": "dist/react-view-router.min.js",
  "module": "esm/index.js",
  "engines": {
    "node": "> 6.9.1"
  },
  "files": [
    "dist",
    "esm",
    "src",
    "build",
    "drawer"
  ],
  "scripts": {
    "build-dev": "cross-env NODE_ENV=development webpack --progresss  --config build/webpack-dev.config.js",
    "build-prod": "cross-env NODE_ENV=production webpack --progresss  --config build/webpack-prod.config.js",
    "build-src": "babel src -d esm",
    "build-drawer": "babel drawer/src -d drawer/esm",
    "build": "npm run build-src && npm run build-drawer",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gxlmyacc/react-view-router/tree/2.x"
  },
  "author": "gxlmyacc",
  "dependencies": {
    "history-fix": "^4.10.1",
    "path-to-regexp": "^3.0.0"
  },
